{
  "name": "@tomsd/mongodbatlas",
  "version": "2.0.0",
  "description": "It's a library for MongoDB Atlas.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "format": "npm run format:src && npm run format:test",
    "format:test": "prettier --write __test__/**/*.ts",
    "format:src": "prettier --write src/**/*.ts",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "ESLINT_USE_FLAT_CONFIG=false eslint src/**/*.ts",
    "lint:test": "ESLINT_USE_FLAT_CONFIG=false eslint __test__/**/*.ts",
    "prepare": "husky",
    "serve:doc": "mdbook --serve --directory docs",
    "test": "vitest"
  },
  "lint-staged": {
    "__test__/**/*.ts": [
      "npm run lint:test",
      "npm run format:test"
    ],
    "src/**/*.ts": [
      "npm run lint:src",
      "npm run format:src"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsdoo/mongodbatlas.git"
  },
  "keywords": [
    "mongodbatlas"
  ],
  "author": "tom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomsdoo/mongodbatlas/issues"
  },
  "homepage": "https://github.com/tomsdoo/mongodbatlas#readme",
  "devDependencies": {
    "@tomsd/md-book": "1.2.0",
    "@types/node": "22.10.0",
    "@typescript-eslint/eslint-plugin": "8.14.0",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.13.2",
    "eslint-plugin-promise": "7.1.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "ts-node": "10.9.2",
    "tsup": "8.3.5",
    "typescript": "5.7.2",
    "vitest": "2.1.5"
  },
  "dependencies": {
    "digest-fetch": "3.1.1"
  }
}
