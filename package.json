{
  "name": "@tomsd/mongodbatlas",
  "version": "2.0.1",
  "description": "It's a library for MongoDB Atlas.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "lint": "biome check",
    "prepare": "husky",
    "serve:doc": "mdbook --serve --directory docs",
    "test": "vitest"
  },
  "lint-staged": {
    "__test__/**/*.ts": [
      "npm run lint"
    ],
    "src/**/*.ts": [
      "npm run lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsdoo/mongodbatlas.git"
  },
  "keywords": [
    "mongodbatlas"
  ],
  "author": "tom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomsdoo/mongodbatlas/issues"
  },
  "homepage": "https://github.com/tomsdoo/mongodbatlas#readme",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tomsd/md-book": "1.3.3",
    "@types/node": "22.10.0",
    "husky": "9.1.7",
    "lint-staged": "15.2.10",
    "ts-node": "10.9.2",
    "tsup": "8.3.5",
    "typescript": "5.7.2",
    "vitest": "2.1.5"
  },
  "dependencies": {
    "digest-fetch": "3.1.1"
  }
}
