{
  "name": "@tomsd/mongodbatlas",
  "version": "1.0.1",
  "description": "It's a library for MongoDB Atlas.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "format": "npm run format:src && npm run format:test",
    "format:test": "prettier --write __test__/**/*.ts",
    "format:src": "prettier --write src/**/*.ts",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "eslint src/**/*.ts",
    "lint:test": "eslint __test__/**/*.ts",
    "prepare": "husky",
    "serve:doc": "mdbook --serve --directory docs",
    "test": "vitest"
  },
  "lint-staged": {
    "__test__/**/*.ts": [
      "npm run lint:test",
      "npm run format:test"
    ],
    "src/**/*.ts": [
      "npm run lint:src",
      "npm run format:src"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsdoo/mongodbatlas.git"
  },
  "keywords": [
    "mongodbatlas"
  ],
  "author": "tom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomsdoo/mongodbatlas/issues"
  },
  "homepage": "https://github.com/tomsdoo/mongodbatlas#readme",
  "devDependencies": {
    "@tomsd/md-book": "1.2.0",
    "@types/node": "22.9.0",
    "@typescript-eslint/eslint-plugin": "8.14.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.13.2",
    "eslint-plugin-promise": "7.1.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "ts-node": "10.9.2",
    "typescript": "5.6.3",
    "vitest": "2.1.5"
  },
  "dependencies": {
    "digest-fetch": "2.0.1"
  }
}
