{
  "name": "@tomsd/mongodbatlas",
  "version": "1.0.1",
  "description": "It's a library for MongoDB Atlas.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "format": "npm run format:src && npm run format:test",
    "format:test": "prettier --write __test__/**/*.ts",
    "format:src": "prettier --write src/**/*.ts",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "eslint src/**/*.ts",
    "lint:test": "eslint __test__/**/*.ts",
    "prepare": "husky install",
    "serve:doc": "mdbook --serve --directory docs",
    "test": "vitest"
  },
  "lint-staged": {
    "__test__/**/*.ts": [
      "npm run lint:test",
      "npm run format:test"
    ],
    "src/**/*.ts": [
      "npm run lint:src",
      "npm run format:src"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsdoo/mongodbatlas.git"
  },
  "keywords": [
    "mongodbatlas"
  ],
  "author": "tom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomsdoo/mongodbatlas/issues"
  },
  "homepage": "https://github.com/tomsdoo/mongodbatlas#readme",
  "devDependencies": {
    "@tomsd/md-book": "0.18.0",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "23.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.6.0",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.2",
    "lint-staged": "13.1.0",
    "prettier": "2.8.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "vitest": "2.1.5"
  },
  "dependencies": {
    "digest-fetch": "2.0.1",
    "node-fetch": "2.6.7"
  }
}
